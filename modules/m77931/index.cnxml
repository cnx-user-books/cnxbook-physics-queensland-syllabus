<m:mn>1</m:mn>
</m:mrow>
</m:mrow>
</m:msup>
</m:mrow>
<m:mo stretchy="false">(</m:mo>
<m:mrow>
<m:msub>
<m:mi>v</m:mi>
<m:mrow>
<m:mi>y</m:mi>
</m:mrow>
</m:msub>
<m:mo stretchy="false">/</m:mo>
<m:msub>
<m:mi>v</m:mi>
<m:mrow>
<m:mi>x</m:mi>
</m:mrow>
</m:msub>
</m:mrow>
<m:mo stretchy="false">)</m:mo>
