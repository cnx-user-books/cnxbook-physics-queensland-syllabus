</m:mrow>
<m:mrow>
<m:mn>2</m:mn>
</m:mrow>
</m:msubsup>
<m:mo stretchy="false">+</m:mo>
<m:msubsup>
<m:mi>A</m:mi>
<m:mrow>
<m:mi>y</m:mi>
</m:mrow>
<m:mrow>
<m:mn>2</m:mn>
</m:mrow>
</m:msubsup>
